{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmit": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "core/*": [ "./src/modules/core/*" ],
      "system/*": [ "./src/system/*" ],
      "locker-cluster/*": [ "./src/modules/locker-cluster/*" ]
    }
  },
  "include": [
    "./src/**/*"
  ]
}